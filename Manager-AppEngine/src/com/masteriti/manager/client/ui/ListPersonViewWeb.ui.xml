<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.masteriti.manager.client.ui" xmlns:p2="urn:import:com.google.gwt.user.cellview.client">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="100%">
			<g:Cell horizontalAlignment="ALIGN_CENTER" verticalAlignment="ALIGN_MIDDLE" height="30px">
				<g:Label text="Person List"/>
			</g:Cell>
			<g:Cell width="">
				<g:HorizontalPanel spacing="1">
					<g:Button ui:field="btnList">List</g:Button>
					<g:Button ui:field="btnAdd">Add</g:Button>
					<g:Button ui:field="btnEdit">Edit</g:Button>
					<g:Button ui:field="btnDelete">Delete</g:Button>
				</g:HorizontalPanel>
			</g:Cell>
			<p1:ShowMorePagerPanel/>
			<p2:CellList ui:field="cellList"/>
		</g:VerticalPanel>
		<g:Label ui:field="lblStatus"/>
	</g:HTMLPanel>
</ui:UiBinder> 